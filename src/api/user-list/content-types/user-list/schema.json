{
  "kind": "collectionType",
  "collectionName": "user_lists",
  "info": {
    "singularName": "user-list",
    "pluralName": "user-lists",
    "displayName": "userlist"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "rollNo": {
      "type": "biginteger",
      "required": true,
      "unique": true
    },
    "course": {
      "type": "string"
    },
    "userType": {
      "type": "string",
      "default": "student"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
