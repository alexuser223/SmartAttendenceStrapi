{
  "kind": "collectionType",
  "collectionName": "user_lists",
  "info": {
    "singularName": "user-list",
    "pluralName": "user-lists",
    "displayName": "User List (Student Profile)",
    "description": "Stores student academic, contact, and personal details"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },

    "rollNumber": {
      "type": "string",
      "required": true
    },

    "email": {
      "type": "email"
    },

    "userType": {
      "type": "string",
      "default": "student"
    },

    "department": {
      "type": "string"
    },

    "course": {
      "type": "string"
    },

    "semester": {
      "type": "integer"
    },

    "phone": {
      "type": "string"
    },

    "emergencyContact": {
      "type": "string"
    },

    "dob": {
      "type": "date"
    },

    "gender": {
      "type": "enumeration",
      "enum": ["Male", "Female", "Other"]
    },

    "address": {
      "type": "text"
    },

    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "required": true
    }
  }
}
